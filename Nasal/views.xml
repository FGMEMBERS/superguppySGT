<?xml version="1.0"?>

<PropertyList>

  <chase-distance-m type="double" archive="y"> -50 </chase-distance-m>

  <view n="100">
    <name>Passanger Seat View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
        <x-offset-m archive="y">   0.509 </x-offset-m> <!--Right-->
        <y-offset-m archive="y">   4.079 </y-offset-m> <!--Up 0.005 - 2.8 -->
        <z-offset-m archive="y"> -11.681 </z-offset-m> <!--Back-->
      <pitch-offset-deg>-15</pitch-offset-deg>
      <default-field-of-view-deg>60</default-field-of-view-deg>
      <dynamic-view type="bool">true</dynamic-view>
    </config>
  </view>

  <view n="101">
    <name>Tail Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <default-field-of-view-deg type="double">30</default-field-of-view-deg>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y"> 0.000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y"> 2.000 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 9.227 </z-offset-m> <!--Back-->
    </config>
  </view>

