  <!-- YOKES -->
  <animation>
    <type>rotate</type>
    <object-name>YokegaucheCol</object-name>
    <object-name>Yokegauche</object-name>
    <property>controls/flight/elevator</property>
    <factor> -3 </factor>
    <center>
      <x-m> -12.200 </x-m>
      <y-m>   0.000 </y-m>
      <z-m>   2.920 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Yokegauche</object-name>
    <property>controls/flight/aileron</property>
    <factor> -60 </factor>
    <center>
      <x-m> -12.200 </x-m>
      <y-m>  -0.527 </y-m>
      <z-m>   3.746 </z-m>
    </center>
